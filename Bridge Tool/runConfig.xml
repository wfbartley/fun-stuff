<run xmlns="http://www.wbartley.com/BridgeTool"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.wbartley.com/BridgeTool bridgetool.xsd"
     runLength="500"
     specificCards="N:SK,E:SAQT76HQ9DAKC7653"
     dumpEachSample="false"
     includePbnHeader="false"
     includeDate="true"
     includeEventAndSite="true"
     includePlayerInfo="true"
     includeScoring="true"
     includeResultInfo="false"
     includeDdAnalysis="false"
     dealer="useBoardNumber"
     vulnerability="useBoardNumber">
    <constraint>
   		<AND>
   			<HCP dir="N" min="6" max="10"/>
   			<DIST dir="N" suit="S" min="6" max="6"/>
   			<DIST dir="N" suit="H" max="3"/>
   			<DIST dir="N" suit="D" max="3"/>
   			<DIST dir="N" suit="C" max="3"/>
   			<MAKES contract="3N" dirs="E"/>
   		</AND>
    </constraint>
    <collectStats>
        <CONTRACT dir="N" contract="2S"/>
        <POINTS dir="W"/>
        <DIST dir="W"/>
    </collectStats>
</run>